#!/bin/sh
Symbols="
–  | -
dʒ | dj
n̪  | nth
tʃ | ch
·  | .
ə  | uh
ʌ  | 'uh
ä  | a
ɛ - | ae
æ  | ae
d̪  | dth
ð  | dth
ɾ̃  | nn
ɳɫ | nl
əɻ |ur
ā | ei
ɖɻ | dr
ɽɻ | ddr
ɚ | ur
ɑ˞ | ar
ɔɻ | or
ē  | ee
ī  | ai
ŋ  | ng
oʊ  | ow
aʊ | aw
ɔ  | o
ɔɪ | oi
ɛ  | eh
ɪ  | ih
ɫ  | dark l
ɱ  | mf
ɸ  | f]
ɹ  | rr
ɾ  | dd
ʃ  | sh
ʊ  | uh
ʒ  | zh
ʔ  | glottal ?
ʰ  | h_
β  | v]
θ  | th
—  | --
→  | ->
ŋ̚ | ng]
æ̃ | ae~
ɔ̃ | o~
ɪ̃ | ih~
b̚ | b]
ɻɖɻ̩ | rdr
ɻɳɻ̩ | rnr
ɻ̩ɳɖ | rnd
ɫɻ̩ɳɖ | lrnd
ɻɽ | rdd
b̥ɹ̩ | br
ɳɖ | nd
n̩ | n
ɻ | rr
ɖ | dd
ɳ | nn
ɽ | dd
ɨ | ih-
ʉ | uh-
ɵ | u
o͞o | uu
"
echo "$Symbols" | dmenu -i -l 10 | awk '{print $1}' | tr -d '\n' | xclip -selection clipboard
notify-send "$(xclip -o -selection clipboard) copied to clipboard"
