#!/bin/sh
Symbols="
–  | -
dʒ | dj
n̪  | nth
tʃ | ch
·  | .
ə  | uh
ä  | a
ɛ - | ae
æ  | ae
d̪  | dth
ð  | dth
ā | ei
ē  | ee
ī  | ai
ŋ  | ng
ow  | ow
aw | aw
ɔ  | o
ɔɪ | oi
ɛ  | eh
ɪ  | ih
ɫ  | dark l
ɱ  | mf
ɸ  | f]
ɹ  | rr
ɾ  | dd
ʃ  | sh
ʊ  | uh
ʒ  | zh
ʔ  | glottal ?
ʰ  | h_
β  | v]
θ  | th
—  | --
→  | ->
"
echo "$Symbols" | dmenu -i -l 10 | awk '{print $1}' | tr -d '\n' | xclip -selection clipboard
notify-send "$(xclip -o -selection clipboard) copied to clipboard"
